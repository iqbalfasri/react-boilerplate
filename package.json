{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "basic react boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest ./src",
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iqbalfasri/react-boilerplate.git"
  },
  "author": "Iqbal Fasri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iqbalfasri/react-boilerplate/issues"
  },
  "homepage": "https://github.com/iqbalfasri/react-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^22.1.4"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
